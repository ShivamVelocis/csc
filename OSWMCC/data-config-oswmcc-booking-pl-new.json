                            {
                                "variable": "approvedBy",
                                "value": {
                                    "path": "$.approvedBy.approvedBy"
                                }
                            },
                            {
                                "variable": "approvedByRole",
                                "value": {
                                    "path": "$.approvedBy.role"
                                }
                            },
                            {
                                "variable": "municipalityContact",
                                "value": {
                                    "path": "$.tenantInfo.contactNumber"
                                }
                            },
                            {
                                "variable": "municipalityWebSite",
                                "value": {
                                    "path": "$.tenantInfo.webSite"
                                }
                            }
